{
  "name": "@sunsetglow/vue-pdf-viewer",
  "version": "0.0.1",
  "author": "wyt",
  "description": "vue pdf viewer",
  "keywords": [
    "vue2",
    "vue3",
    "pdf",
    "viewer"
  ],
  "private": false,
  "license": "ISC",
  "types": "./dist/types/index.d.ts",
  "main": "./dist/pdf-view.js",
  "type": "module",
  "files": [
    "/dist"
  ],
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "scripts": {
    "dev": "vite",
    "build:types": "tsc -p ./tsconfig.types.json -emitDeclarationOnly -esModuleInterop",
    "build": "vue-tsc && vite build && pnpm build:types",
    "preview": "vite preview"
  },
  "dependencies": {
    "@ant-design/icons-vue": "^7.0.1",
    "@types/node": "^20.14.9",
    "ant-design-vue": "^4.2.1",
    "pdfjs-dist": "^4.2.67",
    "vite-plugin-static-copy": "^1.0.6",
    "vue": "^3.4.21"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^5.0.4",
    "typescript": "^5.2.2",
    "vite": "^5.2.0",
    "vue-tsc": "^2.0.6"
  }
}
